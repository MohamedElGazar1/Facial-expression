{
    "builds": [{
        "src": "./main.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9.0" }
    }],
    "routes": [
        {
            "src": "/predict",
            "dest": "./main.py"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "dest": "./main.py"
        }
    ]
}
